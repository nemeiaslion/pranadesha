<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    <application>
        <locale-config>
            <default-locale>pt_BR</default-locale>
            <supported-locale>en_US</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>com.vpaiva.pranadesha.web.UIBundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <message-bundle>com.vpaiva.pranadesha.web.UIBundle</message-bundle>
    </application>
    <navigation-rule>
    	<display-name>site/naturalperson/naturalperson.xhtml</display-name>
    	<from-view-id>/site/naturalperson/naturalperson.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>new</from-outcome>
    		<to-view-id>/site/naturalperson/naturalperson.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<display-name>site/course/course.xhtml</display-name>
    	<from-view-id>/site/course/course.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>showResults</from-outcome>
    		<to-view-id>/site/course/search.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<display-name>site/course/search.xhtml</display-name>
    	<from-view-id>/site/course/search.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>edit</from-outcome>
    		<to-view-id>/site/course/course.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<display-name>site/workshop/workshop.xhtml</display-name>
    	<from-view-id>/site/workshop/workshop.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>showResults</from-outcome>
    		<to-view-id>/site/workshop/search.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<display-name>site/workshop/search.xhtml</display-name>
    	<from-view-id>/site/workshop/search.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>edit</from-outcome>
    		<to-view-id>/site/workshop/workshop.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
</faces-config>
